1.3
log
@Unused class removal.
@
text
@// Edit2.cpp : implementation file
//

#include "stdafx.h"
#include "emule.h"
#include "Edit2.h"

#ifdef _DEBUG
#undef THIS_FILE
static char THIS_FILE[]=__FILE__;
#define new DEBUG_NEW
#endif

// CEdit2

IMPLEMENT_DYNAMIC(CEdit2, CEdit)
CEdit2::CEdit2()
{
	m_cbBackBrush.CreateSolidBrush(RGB(255,255,255));
}

CEdit2::~CEdit2()
{
}


BEGIN_MESSAGE_MAP(CEdit2, CEdit)
	ON_WM_CTLCOLOR_REFLECT()
END_MESSAGE_MAP()



// CEdit2 message handlers


void CEdit2::SetBkColor(COLORREF color)
{
	if(m_cbBackBrush.GetSafeHandle())
		m_cbBackBrush.DeleteObject();
	m_cbBackBrush.CreateSolidBrush(color);
	if(m_hWnd)
		Invalidate();
}

HBRUSH CEdit2::CtlColor(CDC* pDC, UINT nCtlColor)
{
	switch (nCtlColor)
	{
		case CTLCOLOR_EDIT:  
		case CTLCOLOR_STATIC:
		{
			return m_cbBackBrush;
		}
	}
	return NULL;
}
@


