/////////////////////////////////////////////////////////////////////////////
//
// String Table
//

STRINGTABLE 
BEGIN
    IDS_ABOUTBOX            "eMule..."
    IDS_INFLST_USER_COMMUNITY ""
    IDS_HTTPDOWNLOAD_FILESTATUS "%1%2"
    IDS_SOCKETS_INIT_FAILED "windows socket."
    IDS_HTTPDOWNLOAD_CONNECTED "%1"
    IDS_HTTPDOWNLOAD_RESOLVING_NAME ": %1"
    IDS_HTTPDOWNLOAD_RESOLVED_NAME "%1"
    IDS_HTTPDOWNLOAD_CONNECTING "%1"
    IDS_HTTPDOWNLOAD_REDIRECTING "%1"
END

