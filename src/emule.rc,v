BEGIN
    IDS_HTTPDOWNLOAD_GETTING_FILE_INFORMATION ""
    IDS_HTTPDOWNLOAD_FAIL_PARSE_ERROR "url,url: %s"
    IDS_HTTPDOWNLOAD_GENERIC_ERROR ":%1"
    IDS_HTTPDOWNLOAD_FAIL_CONNECT_SERVER ":%1"
    IDS_HTTPDOWNLOAD_INVALID_SERVER_RESPONSE ""
    IDS_HTTPDOWNLOAD_INVALID_HTTP_RESPONSE "HTTP"
