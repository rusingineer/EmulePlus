    IDS_HTTPDOWNLOAD_PERCENTAGE ": %1% /%2"
    IDS_HTTPDOWNLOAD_RETREIVEING_FILE ""
    IDS_HTTPDOWNLOAD_OF     "%1 / %2"
    IDS_HTTPDOWNLOAD_SECONDS "%1"
    IDS_HTTPDOWNLOAD_MINUTES "%1"
    IDS_HTTPDOWNLOAD_MINUTES_AND_SECONDS "%1%2"
    IDS_HTTPDOWNLOAD_BYTES  "%1 "
    IDS_HTTPDOWNLOAD_KILOBYTES "%1 KB"
