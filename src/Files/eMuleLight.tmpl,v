<script language="javascript1.2" type="text/javascript">
<!--
/*************************************************************************
	Pop-it menu - By Dynamic Drive
	For full source code and more DHTML scripts, visit
	http://www.dynamicdrive.com
	This credit MUST stay intact for use
*************************************************************************/
var ie4=document.all;
var ns6=document.getElementById&&!document.all;
var ns4=document.layers;
var ed2kinfo;
var iteminfo;
var removeconfirm;
var confirmtext;

function downmenu(event,admin,finfo,ed2k,downstate,autoa4af,isgetflc,fname,session,filehash)
{
	var code = new Array();
	ed2kinfo = ed2k;
	iteminfo = finfo;

	if(downstate == "hashing")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif" /><img class="menuchecked" src="checked_no.gif" />[FileIsHashing]&nbsp;<\/div>';
	else if(downstate == "error")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif" /><img class="menuchecked" src="checked_no.gif" />[FileIsErroneous]&nbsp;<\/div>';
	else if(downstate == "completing")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif" /><img class="menuchecked" src="checked_no.gif" />[FileIsCompleting]&nbsp;<\/div>';
	else
	{
		code = '<a href="javascript:alert(iteminfo);"><div class="menuitems"><img class="menuimages" src="l_info.gif" /><img class="menuchecked" src="checked_no.gif" />[FileDetails]&nbsp;<\/div><\/a>';
		code += '<a href="javascript:ToClipboard(ed2kinfo);"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif" /><img class="menuchecked" src="checked_no.gif" />[Ed2klink]&nbsp;<\/div><\/a>';

		if(admin == "admin")
		{
			if(downstate == "complete")
				code += '<a href="/?ses=' + session + '&w=transfer&clearcompleted=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_clear.gif" /><img class="menuchecked" src="checked_no.gif" />[ClearCompleted]&nbsp;<\/div><\/a>';
			else
			{
				code += '<a href="/?ses=' + session + '&w=transfer&op=a4af&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_a4af.gif" /><img class="menuchecked" src="checked_no.gif" />[A4AF]&nbsp;<\/div><\/a>';
				code += '<a href="/?ses=' + session + '&w=transfer&op=a4afsamecat&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_a4af.gif" /><img class="menuchecked" src="checked_no.gif" />[A4AFSameCat]&nbsp;<\/div><\/a>';
				if(autoa4af)
					code += '<a href="/?ses=' + session + '&w=transfer&op=noautoa4af&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_auto_a4af.gif" /><img class="menuchecked" src="checked.gif" />[AUTOA4AF]&nbsp;<\/div><\/a>';
				else
					code += '<a href="/?ses=' + session + '&w=transfer&op=autoa4af&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_auto_a4af.gif" /><img class="menuchecked" src="checked_no.gif" />[AUTOA4AF]&nbsp;<\/div><\/a>';
				code += '<a href="/?ses=' + session + '&w=transfer&op=a4afswap&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_a4af.gif" /><img class="menuchecked" src="checked_no.gif" />[A4AFSwap]&nbsp;<\/div><\/a>';
				if(isgetflc == "enabled")
					code += '<a href="/?ses=' + session + '&w=transfer&op=getflc&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_getflc.gif" /><img class="menuchecked" src="checked.gif" />[GetFLC]&nbsp;<\/div><\/a>';
				else if(isgetflc == "disabled")
					code += '<a href="/?ses=' + session + '&w=transfer&op=getflc&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_getflc.gif" /><img class="menuchecked" src="checked_no.gif" />[GetFLC]&nbsp;<\/div><\/a>';
				if(downstate == "paused" || downstate == "stopped")
					code += '<a href="/?ses=' + session + '&w=transfer&op=resume&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_resume.gif" /><img class="menuchecked" src="checked_no.gif" />[Resume]&nbsp;<\/div><\/a>';
				if(downstate != "stopped")
					code += '<a href="/?ses=' + session + '&w=transfer&op=stop&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_stop.gif" /><img class="menuchecked" src="checked_no.gif" />[Stop]&nbsp;<\/div><\/a>';
				if(downstate != "paused")
					code += '<a href="/?ses=' + session + '&w=transfer&op=pause&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_pause.gif" /><img class="menuchecked" src="checked_no.gif" />[Pause]&nbsp;<\/div><\/a>';
				code += '<a href="#" onclick="return cancelfile(\'' + session + '\',\'' + filehash + '\',\'' + fname + '\');"><div class="menuitems"><img class="menuimages" src="l_cancel.gif" /><img class="menuchecked" src="checked_no.gif" />[Cancel]&nbsp;<\/div><\/a>';
				code += '<a href="#" onclick="return filerename(\'' + session + '\',\'' + filehash + '\',\'' + fname + '\');"><div class="menuitems"><img class="menuimages" src="l_rename.gif" /><img class="menuchecked" src="checked_no.gif" />[Rename]&nbsp;<\/div><\/a>';
			}
		}
	}

	showmenu(event,code);
}

function cancelfile(session,filehash,fname)
{
	removeconfirm = '[ConfirmCancel]' + fname;

	if (confirm(removeconfirm))
		window.location='/?ses=' + session + '&w=transfer&op=cancel&file=' + filehash + '[CatSel]';

	return false;
}

function filerename(session,filehash,fname)
{
	var newname = prompt("[Rename]", fname);

	if (newname != fname && newname != null)
		window.location='/?ses=' + session + '&w=transfer&op=rename&name=' + escape(newname) + '&file=' + filehash + '[CatSel]';

	return false;
}

function servermenu(event,admin,ed2k,session,ip,port,isstatic)
{
	var code = new Array();
	ed2kinfo = ed2k;

	code = '<a href="javascript:ToClipboard(ed2kinfo);"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif" /><img class="menuchecked" src="checked_no.gif" />[Ed2klink]&nbsp;<\/div><\/a>';

	if(admin == "admin")
	{
		code += '<a href="/?ses=' + session + '&w=server&c=connect&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_connect.gif" /><img class="menuchecked" src="checked_no.gif" />[Connect]&nbsp;<\/div><\/a>';
		code += '<a href="#" onclick="return removeserver(\'' + session + '\',\'' + ip + '\',\'' + port + '\');"><div class="menuitems"><img class="menuimages" src="l_cancel.gif" /><img class="menuchecked" src="checked_no.gif" />[RemoveServer]&nbsp;<\/div><\/a>';
		if(isstatic)
			code += '<a href="/?ses=' + session + '&w=server&c=removefromstatic&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_static.gif" /><img class="menuchecked" src="checked.gif" />[StaticServer]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=server&c=addtostatic&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_static.gif" /><img class="menuchecked" src="checked_no.gif" />[StaticServer]&nbsp;<\/div><\/a>';
	}

	showmenu(event,code);
}

function removeserver(session,ip,port)
{
	if (confirm("[ConfirmRemove]"))
		window.location='/?ses=' + session + '&w=server&c=remove&ip=' + ip + '&port=' + port + '[CatSel]';

	return false;
}

function sharedmenu(event,admin,ed2k,session,hash,fname,isjumpstart,filestate)
{
	var code = new Array();
	ed2kinfo = ed2k;

	code = '<a href="javascript:ToClipboard(ed2kinfo);"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif" /><img class="menuchecked" src="checked_no.gif" />[Ed2klink]&nbsp;<\/div><\/a>';

	if(admin == "admin")
	{
		if(filestate == "file")
		{
			if(isjumpstart)
				code += '<a href="#" onclick="return removejumpstart(\'' + session + '\',\'' + hash + '\',\'' + fname + '\');"><div class="menuitems"><img class="menuimages" src="l_jumpstart.gif" /><img class="menuchecked" src="checked.gif" />JumpStart&nbsp;<\/div><\/a>';
			else
				code += '<a href="/?ses=' + session + '&w=shared&jumpstart=true&hash=' + hash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_jumpstart.gif" /><img class="menuchecked" src="checked_no.gif" />JumpStart&nbsp;<\/div><\/a>';
		}
	}

	showmenu(event,code);
}

function removejumpstart(session,filehash,fname)
{
	removeconfirm = '[ConfirmJumpstart]' + fname;

	if (confirm(removeconfirm))
		window.location='/?ses=' + session + '&w=shared&jumpstart=false&hash=' + filehash + '[CatSel]';

	return false;
}

function adminmenu(event,admin)
{
	var code = new Array();

	code = '<a href="/?ses=[Session]&amp;w=logout"><div class="menuitems"><img class="menuimages" src="l_logout.gif" alt="[Logout]" /><img class="menuchecked" src="checked_no.gif" />[Logout]&nbsp;<\/div><\/a>';

	if(admin == "admin")
	{
		code += '<a href="#" onclick="return adminconfirm(\'close\')"><div class="menuitems"><img class="menuimages" src="l_close.gif" alt="[Close]" /><img class="menuchecked" src="checked_no.gif" />[Close]&nbsp;<\/div><\/a>';
		code += '<a href="#" onclick="return adminconfirm(\'reboot\')"><div class="menuitems"><img class="menuimages" src="l_reboot.gif" alt="[Reboot]" /><img class="menuchecked" src="checked_no.gif" />[Reboot]&nbsp;<\/div><\/a>';
		code += '<a href="#" onclick="return adminconfirm(\'shutdown\')"><div class="menuitems"><img class="menuimages" src="l_shutdown.gif" alt="[Shutdown]" /><img class="menuchecked" src="checked_no.gif" />[Shutdown]&nbsp;<\/div><\/a>';
	}

	showmenu(event,code);
}

function adminconfirm(type)
{
	if (type == 'close')
		confirmtext = "[ConfirmClose]";
	else if (type == 'reboot')
		confirmtext = "[ConfirmReboot]";
	else if (type == 'shutdown')
		confirmtext = "[ConfirmShutdown]";

	if (confirm(confirmtext))
	{
		alert('/?ses=[Session]&w=' + type);
		window.location = '/?ses=[Session]&w=' + type;
	}

	return false;
}

function upmenu(event,admin,clientextra,userhash)
{
	var code = new Array();

	if(admin == "admin")
	{
		if(clientextra == "friend")
		{
			code = '<a href="/?ses=[Session]&amp;w=transfer&op=removefriend&amp;userhash=' + userhash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_friend.gif" alt="[Friend]" /><img class="menuchecked" src="checked.gif" />[Friend]&nbsp;<\/div><\/a>';
		}
		else
		{
			code = '<a href="/?ses=[Session]&amp;w=transfer&op=addfriend&amp;userhash=' + userhash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_friend.gif" alt="[Friend]" /><img class="menuchecked" src="checked_no.gif" />[Friend]&nbsp;<\/div><\/a>';
		}

		showmenu(event,code);
	}
}

function searchmenu(event,ed2k)
{
	var code = new Array();
	ed2kinfo = ed2k;

	code = '<a href="javascript:ToClipboard(ed2kinfo);"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif" /><img class="menuchecked" src="checked_no.gif" />[Ed2klink]&nbsp;<\/div><\/a>';

	showmenu(event,code);
}

function columnmenu(event,tab,menu, m1,e1,m2,e2,m3,e3,m4,e4,m5,e5,m6,e6,m7,e7,m8,e8,m9,e9,m10,e10,m11,e11)
{
	var code = new Array();
	code = "";

	for (i=1; i<=11; i++)
	{
		try
		{
			var e = eval('e'+i);
			var m = eval('m'+i);
			var j = i - 1;

			if(e != "") { code += '<a href="/?ses=[Session]&amp;w='+tab+'&amp;c='+menu+'&amp;m='+j+'&amp;v=1[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />'+m+'&nbsp;<\/div><\/a>'; }
			else if(m != "") { code += '<a href="/?ses=[Session]&amp;w='+tab+'&amp;c='+menu+'&amp;m='+j+'&amp;v=0[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />'+m+'&nbsp;<\/div><\/a>'; }
		}
		catch(e)
		{
			// The variable doesn't exist
		}
	}

	showmenu(event,code);
}

function catpriomenu(event,admin,session)
{
	var code = new Array();

	if(admin == "admin")
	{
		code = '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=low[CatSel]"><div class="menuitems">&nbsp;[PriorityLow]&nbsp;<\/div><\/a>';
		code += '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=normal[CatSel]"><div class="menuitems">&nbsp;[PriorityNormal]&nbsp;<\/div><\/a>';
		code += '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=high[CatSel]"><div class="menuitems">&nbsp;[PriorityHigh]&nbsp;<\/div><\/a>';
		code += '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=auto[CatSel]"><div class="menuitems">&nbsp;[PriorityAuto]&nbsp;<\/div><\/a>';

		showmenu(event,code);
	}
}

function serverpriomenu(event,admin,session,ip,port,priority)
{
	var code = new Array();

	if(admin == "admin")
	{
		if(priority == "Low")
			code = '<a href="/?ses=' + session + '&w=server&c=priolow&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityLow]&nbsp;<\/div><\/a>';
		else
			code = '<a href="/?ses=' + session + '&w=server&c=priolow&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityLow]&nbsp;<\/div><\/a>';
		if(priority == "Normal")
			code += '<a href="/?ses=' + session + '&w=server&c=prionormal&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityNormal]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=server&c=prionormal&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityNormal]&nbsp;<\/div><\/a>';
		if(priority == "High")
			code += '<a href="/?ses=' + session + '&w=server&c=priohigh&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityHigh]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=server&c=priohigh&ip=' + ip + '&port=' + port + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityHigh]&nbsp;<\/div><\/a>';

		showmenu(event,code);
	}
}

function downpriomenu(event,admin,session,filehash,priority,downstate)
{
	var code = new Array();

	if(downstate == "complete" || downstate == "completing")
		return;

	if(admin == "admin")
	{
		if(priority == "Low")
			code = '<a href="/?ses=' + session + '&w=transfer&op=priolow&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityLow]&nbsp;<\/div><\/a>';
		else
			code = '<a href="/?ses=' + session + '&w=transfer&op=priolow&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityLow]&nbsp;<\/div><\/a>';
		if(priority == "Normal")
			code += '<a href="/?ses=' + session + '&w=transfer&op=prionormal&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityNormal]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=transfer&op=prionormal&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityNormal]&nbsp;<\/div><\/a>';
		if(priority == "High")
			code += '<a href="/?ses=' + session + '&w=transfer&op=priohigh&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityHigh]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=transfer&op=priohigh&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityHigh]&nbsp;<\/div><\/a>';
		if(priority == "Auto")
			code += '<a href="/?ses=' + session + '&w=transfer&op=prioauto&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityAuto]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=transfer&op=prioauto&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityAuto]&nbsp;<\/div><\/a>';

		showmenu(event,code);
	}
}

function sharedpriomenu(event,admin,session,filehash,priority)
{
	var code = new Array();

	if(admin == "admin")
	{
		if(priority == "VeryLow")
			code = '<a href="/?ses=' + session + '&w=shared&prio=verylow&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityVeryLow]&nbsp;<\/div><\/a>';
		else
			code = '<a href="/?ses=' + session + '&w=shared&prio=verylow&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityVeryLow]&nbsp;<\/div><\/a>';
		if(priority == "Low")
			code += '<a href="/?ses=' + session + '&w=shared&prio=low&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityLow]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=shared&prio=low&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityLow]&nbsp;<\/div><\/a>';
		if(priority == "Normal")
			code += '<a href="/?ses=' + session + '&w=shared&prio=normal&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityNormal]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=shared&prio=normal&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityNormal]&nbsp;<\/div><\/a>';
		if(priority == "High")
			code += '<a href="/?ses=' + session + '&w=shared&prio=high&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityHigh]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=shared&prio=high&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityHigh]&nbsp;<\/div><\/a>';
		if(priority == "Release")
			code += '<a href="/?ses=' + session + '&w=shared&prio=release&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityRelease]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=shared&prio=release&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityRelease]&nbsp;<\/div><\/a>';
		if(priority == "Auto")
			code += '<a href="/?ses=' + session + '&w=shared&prio=auto&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif" />[PriorityAuto]&nbsp;<\/div><\/a>';
		else
			code += '<a href="/?ses=' + session + '&w=shared&prio=auto&hash=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif" />[PriorityAuto]&nbsp;<\/div><\/a>';

		showmenu(event,code);
	}
}
