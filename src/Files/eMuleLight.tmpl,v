@d306 1
d310 1
a310 1
function catpriomenu(event,admin,session,priority)
d316 4
a319 16
		if(priority == "Low")
			code += '<a href="/?ses=' + session + '&w=transfer&op=catlow[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityLow]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=transfer&op=catlow[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityLow]&nbsp;</div></a>'
		if(priority == "Normal")
			code += '<a href="/?ses=' + session + '&w=transfer&op=catnormal[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityNormal]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=transfer&op=catnormal[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityNormal]&nbsp;</div></a>'
		if(priority == "High")
			code += '<a href="/?ses=' + session + '&w=transfer&op=cathigh[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityHigh]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=transfer&op=cathigh[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityHigh]&nbsp;</div></a>'
		if(priority == "Auto")
			code += '<a href="/?ses=' + session + '&w=transfer&op=prioauto[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityAuto]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=transfer&op=prioauto[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityAuto]&nbsp;</div></a>'
