var timeoutID;
var timeout = [RefreshVal];

function RefreshPage()
{
	location.href = "/?ses=[Session]&w=[wCommandJ]&scroll="+((document.all)?document.body.scrollTop:window.pageYOffset);
}

function ToggleTimer()
{
	if (timeoutID)
	{
		clearTimeout(timeoutID);
		timeoutID = null;
		document.images['ImgToggleTimer'].src = 'l_timer_off.gif';
		document.images['ImgToggleTimer'].title = '[SetTimerOn]';
	}
	else
	{
		SetTimer();
		if (timeoutID)
		{
			document.images['ImgToggleTimer'].src = 'l_timer.gif';
			document.images['ImgToggleTimer'].title = '[SetTimerOff]';
		}
		else
		{
			document.images['ImgToggleTimer'].src = 'l_timer_off.gif';
			document.images['ImgToggleTimer'].title = '[SetTimerOn]';
		}
	}
}

function SetTimer()
{
	if (timeout != 0)
	{
		timeoutID = setTimeout("RefreshPage()", timeout);
	}
}

if (document.all||document.getElementById)
	SetTimer();
else
	window.onload=SetTimer;
