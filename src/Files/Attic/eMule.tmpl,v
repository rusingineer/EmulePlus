a318 1
function serverpriomenu(event,admin,session,ip,port,priority)
d324 4
a327 12
		if(priority == "Low")
			code += '<a href="/?ses=' + session + '&w=server&c=priolow&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityLow]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=server&c=priolow&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityLow]&nbsp;</div></a>'
		if(priority == "Normal")
			code += '<a href="/?ses=' + session + '&w=server&c=prionormal&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityNormal]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=server&c=prionormal&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityNormal]&nbsp;</div></a>'
		if(priority == "High")
			code += '<a href="/?ses=' + session + '&w=server&c=priohigh&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityHigh]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=server&c=priohigh&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityHigh]&nbsp;</div></a>'
