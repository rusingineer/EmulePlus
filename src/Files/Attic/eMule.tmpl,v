<script language="JavaScript1.2">
function obj(menu) {
	return (navigator.appName == "Microsoft Internet Explorer")?this[menu]:document.getElementById(menu);
}
function searchange1() {
	obj("manual").style.visibility="hidden";
	obj("manual").style.position="absolute";
	obj("manualtag").style.background="#000000";
	obj("server").style.visibility="hidden";
	obj("server").style.position="absolute";
	obj("servertag").style.background="#000000";
	obj("prefs").style.visibility="";
	obj("prefs").style.position="";
	obj("prefstag").style.background="#0066CC";
}
function searchange2() {
	obj("manual").style.visibility="hidden";
	obj("manual").style.position="absolute";
	obj("manualtag").style.background="#000000";
	obj("server").style.visibility="";
	obj("server").style.position="";
	obj("servertag").style.background="#0066CC";
	obj("prefs").style.visibility="hidden";
	obj("prefs").style.position="absolute";
	obj("prefstag").style.background="#000000";

}
function searchange3() {
	obj("manual").style.visibility="";
	obj("manual").style.position="";
	obj("manualtag").style.background="#0066CC";
	obj("server").style.visibility="hidden";
	obj("server").style.position="absolute";
	obj("servertag").style.background="#000000";
	obj("prefs").style.visibility="hidden";
	obj("prefs").style.position="absolute";
	obj("prefstag").style.background="#000000";
}
