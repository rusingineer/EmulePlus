<--TMPL_GRAPHS-->
&nbsp;
<SCRIPT language=JavaScript>
function traceGraph(SpeedString,imageURL,MaxSpeed) {
	document.write('<table cellspacing=0 border=0 cellpadding=0 width="500px"><tr><td valign="bottom" height="150" width="100%"></td>');
	var speed = SpeedString.split(",");
	if(MaxSpeed==0) MaxSpeed = 1; //No divide by 0 !
	for(i=0;i<speed.length;i++)
		{
		speed[i]=new Number(speed[i])
	        size=Math.round(speed[i]*150/MaxSpeed);
	        document.write('<td valign="bottom"><table height="'+ size +'px" width="1px" border=0 cellpadding=0 cellspacing=0><td background="' + imageURL + '" height="100%" width="100%"></td></table></td>');
		}
	document.write('</tr></table>');
}
</SCRIPT>
<table border=0 align=center cellpadding=0 cellspacing=0>
<tr><td style="border-width:1px; border-color:green; border-style:solid;" valign="bottom" height="150px" background="back.gif">
<SCRIPT language=JavaScript>
var dlstring="[GraphDownload]";
traceGraph(dlstring,"stats_down.gif",[MaxDownload]*1024);
</SCRIPT>
</td></tr>
<tr><td align=center><font color=black><font face=Tahoma style="font-size:8pt;">[TxtDownload]: Max [MaxDownload] [KByteSec], [TxtTime]: [ScaleTime]</font><br>&nbsp;</td></tr>
<tr><td style="border-width:1px; border-color:blue; border-style:solid;" valign="bottom" height="150px" background="back.gif">
<SCRIPT language=JavaScript>
var ulstring="[GraphUpload]";
traceGraph(ulstring,"stats_up.gif",[MaxUpload]*1024);
</SCRIPT>
</td></tr>
<tr><td align=center><font color=black><font face=Tahoma style="font-size:8pt;">[TxtUpload]: Max [MaxUpload] [KByteSec], [TxtTime]: [ScaleTime]</font><br>&nbsp;</td></tr>
<tr><td style="border-width:1px; border-color:red; border-style:solid;" valign="bottom" height="150px" background="back.gif">
<SCRIPT language=JavaScript>
var ulstring="[GraphConnections]";
traceGraph(ulstring,"stats_con.gif",[MaxConnections]);
</SCRIPT>
</td></tr>
<tr><td align=center><font color=black><font face=Tahoma style="font-size:8pt;">[TxtConnections]: Max [MaxConnections], [TxtTime]: [ScaleTime]</font></td></tr>
</table>
