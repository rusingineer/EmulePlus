<script language="JavaScript1.2">
function obj(menu) {
	return (navigator.appName == "Microsoft Internet Explorer")?this[menu]:document.getElementById(menu);
}
function searchange1() {
	obj("search").style.visibility="";
	obj("search").style.position="";
	obj("searchtag").style.background="#0066CC";
	obj("filedonkey").style.visibility="hidden";
	obj("filedonkey").style.position="absolute";
	obj("filedonkeytag").style.background="#000000";
}
function searchange2() {
	obj("search").style.visibility="hidden";
	obj("search").style.position="absolute";
	obj("searchtag").style.background="#000000";
	obj("filedonkey").style.visibility="";
	obj("filedonkey").style.position="";
	obj("filedonkeytag").style.background="#0066CC";
}
function searchange3() {
	obj("search").style.visibility="hidden";
	obj("search").style.position="absolute";
	obj("searchtag").style.background="#000000";
	obj("filedonkey").style.visibility="hidden";
	obj("filedonkey").style.position="absolute";
	obj("filedonkeytag").style.background="#000000";
}
</script>
