//Pop-it menu- By Dynamic Drive
//For full source code and more DHTML scripts, visit http://www.dynamicdrive.com
//This credit MUST stay intact for use

var ie4=document.all
var ns6=document.getElementById&&!document.all
var ns4=document.layers
var ed2kinfo;
var iteminfo;
var removeconfirm;
var confirmclose;
var confirmshutdown;

function downmenu(event,admin,finfo,ed2k,downstate,autoa4af,scb,fname,session,filehash)
{
	var code = new Array()
	ed2kinfo = ed2k
	iteminfo = finfo
	removeconfirm = '[ConfirmCancel]' + fname
			
	if(downstate == "hashing")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif"><img class="menuchecked" src="checked_no.gif">[FileIsHashing]&nbsp;</div>'
	else if(downstate == "error")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif"><img class="menuchecked" src="checked_no.gif">[FileIsErroneous]&nbsp;</div>'
	else if(downstate == "completing")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif"><img class="menuchecked" src="checked_no.gif">[FileIsCompleting]&nbsp;</div>'
	else
	{
		code = '<a href="javascript:alert(iteminfo);"><div class="menuitems"><img class="menuimages" src="l_info.gif"><img class="menuchecked" src="checked_no.gif">[FileDetails]&nbsp;</div></a>'
		code += '<a href="javascript:ToClipboard(ed2kinfo);"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif"><img class="menuchecked" src="checked_no.gif">[Ed2klink]&nbsp;</div></a>'

		if(admin == "admin")
		{
			if(downstate == "complete")
				code += '<a href="/?ses=' + session + '&w=transfer&clearcompleted=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_clear.gif"><img class="menuchecked" src="checked_no.gif">[ClearCompleted]&nbsp;</div></a>'
			else
			{
				code += '<a href="/?ses=' + session + '&w=transfer&op=a4af&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_a4af.gif"><img class="menuchecked" src="checked_no.gif">[A4AF]&nbsp;</div></a>'
				if(autoa4af)
					code += '<a href="/?ses=' + session + '&w=transfer&op=noautoa4af&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_auto_a4af.gif"><img class="menuchecked" src="checked.gif">[AUTOA4AF]&nbsp;</div></a>'
				else
					code += '<a href="/?ses=' + session + '&w=transfer&op=autoa4af&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_auto_a4af.gif"><img class="menuchecked" src="checked_no.gif">[AUTOA4AF]&nbsp;</div></a>'
				if(scb)
					code += '<a href="/?ses=' + session + '&w=transfer&op=compressed&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_dropscb.gif"><img class="menuchecked" src="checked.gif">[DropSCB]&nbsp;</div></a>'
				else
					code += '<a href="/?ses=' + session + '&w=transfer&op=compressed&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_dropscb.gif"><img class="menuchecked" src="checked_no.gif">[DropSCB]&nbsp;</div></a>'
				code += '<a href="/?ses=' + session + '&w=transfer&op=noneededsources&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_dropnns.gif"><img class="menuchecked" src="checked_no.gif">[NoNeededSources]&nbsp;</div></a>'
				if(downstate == "paused" || downstate == "stopped")
					code += '<a href="/?ses=' + session + '&w=transfer&op=resume&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_resume.gif"><img class="menuchecked" src="checked_no.gif">[Resume]&nbsp;</div></a>'
				else
				{
					code += '<a href="/?ses=' + session + '&w=transfer&op=stop&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_stop.gif"><img class="menuchecked" src="checked_no.gif">[Stop]&nbsp;</div></a>'
					code += '<a href="/?ses=' + session + '&w=transfer&op=pause&file=' + filehash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_pause.gif"><img class="menuchecked" src="checked_no.gif">[Pause]&nbsp;</div></a>'
				}
				code += '<a href="/?ses=' + session + '&w=transfer&op=cancel&file=' + filehash + '[CatSel]" onclick="return confirm(removeconfirm)"><div class="menuitems"><img class="menuimages" src="l_cancel.gif"><img class="menuchecked" src="checked_no.gif">[Cancel]&nbsp;</div></a>'
			}
		}
	}

	showmenu(event,code)
}

function servermenu(event,admin,ed2k,session,ip,port,isstatic)
{
	var code = new Array()
	ed2kinfo = ed2k
	
	code = '<a href="javascript:ToClipboard(ed2kinfo);"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif"><img class="menuchecked" src="checked_no.gif">[Ed2klink]&nbsp;</div></a>'

	if(admin == "admin")
	{
		code += '<a href="/?ses=' + session + '&w=server&c=connect&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuimages" src="l_connect.gif"><img class="menuchecked" src="checked_no.gif">[Connect]&nbsp;</div></a>'
		code += '<a href="/?ses=' + session + '&w=server&c=remove&ip=' + ip + '&port=' + port + '" onclick="return confirm(\'[ConfirmRemove]\')' + '"><div class="menuitems"><img class="menuimages" src="l_cancel.gif"><img class="menuchecked" src="checked_no.gif">[RemoveServer]&nbsp;</div></a>'
		if(isstatic)
			code += '<a href="/?ses=' + session + '&w=server&c=removefromstatic&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuimages" src="l_static.gif"><img class="menuchecked" src="checked.gif">[StaticServer]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=server&c=addtostatic&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuimages" src="l_static.gif"><img class="menuchecked" src="checked_no.gif">[StaticServer]&nbsp;</div></a>'
	}

	showmenu(event,code)
}

function sharedmenu(event,admin,ed2k,session,hash,fname,isjumpstart,filestate)
{
	var code = new Array()
	ed2kinfo = ed2k
	removeconfirm = '[ConfirmJumpstart]' + fname
	
	code = '<a href="javascript:ToClipboard(ed2kinfo);"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif"><img class="menuchecked" src="checked_no.gif">[Ed2klink]&nbsp;</div></a>'

	if(admin == "admin")
	{
		if(filestate == "file")
		{
			if(isjumpstart)
				code += '<a href="/?ses=' + session + '&w=shared&jumpstart=false&hash=' + hash + '" onclick="return confirm(removeconfirm)"><div class="menuitems"><img class="menuimages" src="l_jumpstart.gif"><img class="menuchecked" src="checked.gif">[Jumpstart]&nbsp;</div></a>'
			else
				code += '<a href="/?ses=' + session + '&w=shared&jumpstart=true&hash=' + hash + '"><div class="menuitems"><img class="menuimages" src="l_jumpstart.gif"><img class="menuchecked" src="checked_no.gif">[Jumpstart]&nbsp;</div></a>'
		}
	}

	showmenu(event,code)
}

function adminmenu(event,admin)
{
	var code = new Array()

	confirmclose = '[ConfirmClose]'
	confirmshutdown = '[ConfirmShutdown]'

	code += '<a href="javascript:ShowHide(' + "'" + 'ed2k_open' + "'" + ');"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif" alt="[Ed2klink]"><img class="menuchecked" src="checked_no.gif">[Ed2klink]&nbsp;</div></a>'
	code += '<a href="/?ses=[Session]&amp;w=logout"><div class="menuitems"><img class="menuimages" src="l_logout.gif" alt="[Logout]"><img class="menuchecked" src="checked_no.gif">[Logout]&nbsp;</div></a>'

	if(admin == "admin")
	{
		code += '<a href="/?ses=[Session]&amp;w=close" onclick="return confirm(confirmclose)"><div class="menuitems"><img class="menuimages" src="l_close.gif" alt="[Close]"><img class="menuchecked" src="checked_no.gif">[Close]&nbsp;</div></a>'
		code += '<a href="/?ses=[Session]&amp;w=shutdown" onclick="return confirm(confirmshutdown)"><div class="menuitems"><img class="menuimages" src="l_shutdown.gif" alt="[Shutdown]"><img class="menuchecked" src="checked_no.gif">[Shutdown]&nbsp;</div></a>'
	}

	showmenu(event,code)
}

