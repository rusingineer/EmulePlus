function upmenu(event,admin,clientextra,userhash)
{
	var code = new Array()
	
	if(admin == "admin")
	{
		if(clientextra == "friend")
		{
			code += '<a href="/?ses=[Session]&amp;w=transfer&op=removefriend&amp;userhash=' + userhash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_friend.gif" alt="[Friend]"><img class="menuchecked" src="checked.gif">[Friend]&nbsp;</div></a>'
		}
		else
		{
			code += '<a href="/?ses=[Session]&amp;w=transfer&op=addfriend&amp;userhash=' + userhash + '[CatSel]"><div class="menuitems"><img class="menuimages" src="l_friend.gif" alt="[Friend]"><img class="menuchecked" src="checked_no.gif">[Friend]&nbsp;</div></a>'
		}
	}

	showmenu(event,code)
}

function searchmenu(event,ed2k)
{
	var code = new Array()
	
	code += '<a href="' + ed2k + '"><div class="menuitems"><img class="menuimages" src="l_ed2klink.gif" alt="[Ed2klink]"><img class="menuchecked" src="checked_no.gif">[Ed2klink]&nbsp;</div></a>'

	showmenu(event,code)
}

function columnmenu(event,tab,menu, m1,e1,m2,e2,m3,e3,m4,e4,m5,e5,m6,e6,m7,e7,m8,e8,m9,e9,m10,e10,m11,e11)
{
	var code = new Array();

	for (i=1; i<=11; i++)
	{
		try
		{
			var e = eval('e'+i);
			var m = eval('m'+i);
			var j = i - 1;

			if(e != "") { code += '<a href="/?ses=[Session]&amp;w='+tab+'&amp;c='+menu+'&amp;m='+j+'&amp;v=1[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked.gif">'+m+'&nbsp;</div></a>' }
			else if(m != "") { code += '<a href="/?ses=[Session]&amp;w='+tab+'&amp;c='+menu+'&amp;m='+j+'&amp;v=0[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">'+m+'&nbsp;</div></a>' }
		}
		catch(e)
		{
			// Doesn't exist that variable
		}
	}

	showmenu(event,code);
}

function catpriomenu(event,admin,session)
{
	var code = new Array()

	if(admin == "admin")
	{
			code += '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=low[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityLow]&nbsp;</div></a>'
			code += '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=normal[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityNormal]&nbsp;</div></a>'
			code += '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=high[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityHigh]&nbsp;</div></a>'
			code += '<a href="/?ses=' + session + '&w=transfer&c=menuprio&p=auto[CatSel]"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityAuto]&nbsp;</div></a>'
	}

	showmenu(event,code)
}

function serverpriomenu(event,admin,session,ip,port,priority)
{
	var code = new Array()

	if(admin == "admin")
	{
		if(priority == "Low")
			code += '<a href="/?ses=' + session + '&w=server&c=priolow&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityLow]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=server&c=priolow&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityLow]&nbsp;</div></a>'
		if(priority == "Normal")
			code += '<a href="/?ses=' + session + '&w=server&c=prionormal&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityNormal]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=server&c=prionormal&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityNormal]&nbsp;</div></a>'
		if(priority == "High")
			code += '<a href="/?ses=' + session + '&w=server&c=priohigh&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked.gif">[PriorityHigh]&nbsp;</div></a>'
		else
			code += '<a href="/?ses=' + session + '&w=server&c=priohigh&ip=' + ip + '&port=' + port + '"><div class="menuitems"><img class="menuchecked" src="checked_no.gif">[PriorityHigh]&nbsp;</div></a>'
	}

	showmenu(event,code)
}

