function centerCSS(item_1,item_2) {
	var center_height = document.getElementById(item_2).scrollHeight;
	document.getElementById(item_1).style.height = center_height + "px";
	var center_marginTop = center_height / 2;
	document.getElementById(item_1).style.marginTop = "-" + center_marginTop + "px";

	var center_width = document.getElementById(item_2).scrollWidth;
	document.getElementById(item_1).style.width = center_width + "px";
	var center_marginLeft = center_width / 2;
	document.getElementById(item_1).style.marginLeft = "-" + center_marginLeft + "px";
}

function onLoadInit() {
	document.login.p.focus();
	centerCSS('center','login_dialog');
}
