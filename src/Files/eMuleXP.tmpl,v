@a539 10
function fakecheck(event,fname,filehash,fsize)
{
	var code = new Array()

	code = '<a href="http://donkeyfakes.gambri.net/fakecheck/update/fakecheck.php?hash=' + filehash + '[CatSel]" target="_blank"><div class="menuitems"><img class="menuimages" src="l_donkeyfakes.gif"><img class="menuchecked" src="checked_no.gif">eDonkey Fake Search&nbsp;</div></a>'
	code += '<a href="http://www.filedonkey.com/search.html?pattern=' + fname + '[CatSel]" target="_blank"><div class="menuitems"><img class="menuimages" src="l_filedonkey.gif"><img class="menuchecked" src="checked_no.gif">FileDonkey Fake Search&nbsp;</div></a>'

	showmenu(event,code)
}

