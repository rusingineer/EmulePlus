<script language="javascript1.2" type="text/javascript">
function obj(menu) {
	return (navigator.appName == "Microsoft Internet Explorer")?this[menu]:document.getElementById(menu);
}
function searchange1() {
	obj("manual").style.display="none";
	obj("manualtag").style.background="#000000";
	obj("server").style.display="none";
	obj("servertag").style.background="#000000";
	obj("prefs").style.display="";
	obj("prefstag").style.background="#005784";
}
function searchange2() {
	obj("manual").style.display="none";
	obj("manualtag").style.background="#000000";
	obj("server").style.display="";
	obj("servertag").style.background="#005784";
	obj("prefs").style.display="none";
	obj("prefstag").style.background="#000000";

}
function searchange3() {
	obj("manual").style.display="";
	obj("manualtag").style.background="#005784";
	obj("server").style.display="none";
	obj("servertag").style.background="#000000";
	obj("prefs").style.display="none";
	obj("prefstag").style.background="#000000";
}
