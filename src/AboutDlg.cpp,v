d357 1
a357 1
	m_ctrlCredits.Create(NULL, WS_CHILD | WS_VISIBLE /*| WS_BORDER*/, rCredits, this);
a421 14

void CAboutDlg::DrawCreditsBackground(CDC *pDC, RECT rect, BOOL bAnimate, DWORD lParam)
{
	CAboutDlg *pDlg = (CAboutDlg*)lParam;

	CRect rCtrl;
	pDlg->m_ctrlCredits.GetWindowRect(rCtrl);
	pDlg->ScreenToClient(&rCtrl);

	rect.left -= rCtrl.left;
	rect.top  -= rCtrl.top;
	pDC->DrawState(CPoint(rect.left,rect.top), CSize(rect.right-rect.left,rect.bottom-rect.top),
					pDlg->m_imgBackground, DST_BITMAP, NULL);
}
